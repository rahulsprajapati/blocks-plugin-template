{
  "name": "rahulsprajapati/blocks-plugin-template",
  "description": "Gutenberg Blocks Plugin Template",
  "authors": [
    {
      "name": "Rahul Prajapati",
      "email": "rahul.prajapati@live.in",
      "homepage": "https://github.com/rahulsprajapati",
      "role": "Developer"
    }
  ],
  "type": "wordpress-plugin",
	"require": {
    "php": ">=8.0"
  },
	"autoload": {
    "psr-4": {
      "BlocksPluginTemplate\\": "src/"
    }
  },
  "prefer-stable": true,
  "homepage": "https://github.com/rahulsprajapati/blocks-plugin-template",
  "support": {
    "issues": "https://github.com/rahulsprajapati/blocks-plugin-template/issues",
    "source": "https://github.com/rahulsprajapati/blocks-plugin-template/"
  },
  "minimum-stability": "dev",
  "require-dev": {
		"10up/phpcs-composer": "dev-master",
    "phpunit/phpunit": "9.5.5",
		"yoast/phpunit-polyfills": "1.0.3",
    "phpcompatibility/php-compatibility": "^9.3"
  },
	"scripts": {
    "lint": "./vendor/bin/phpcs .",
    "lint-fix": "./vendor/bin/phpcbf ."
  },
	"config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
